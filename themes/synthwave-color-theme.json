// Color Palette for SynthWave 84 theme
/* Pink: #F92672
   Blue: #66D9EF
   Green: #4ade80
   Yellow: #E6DB74
   Orange: #FD971F
   Purple: #AE81FF
   Red: #FF5555
   White: #F8F8F2
   Black: #0A0A0A
   Gray: #181A1F
   Apple Intelligence Placeholder: #fc0303
*/

{
  "name": "SynthWave 84",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "activityBar.background": "#0A0A0A",
    "activityBar.foreground": "#fff",
    "activityBar.activeBorder": "#F92672",
    "activityBar.activeFocusBorder": "#F92672",
    "activityBarBadge.background": "#F92672",
    "activityBarBadge.foreground": "#F8F8F2",
    "badge.background": "#66D9EF",
    "badge.foreground": "#fff",
    "button.background": "#4ade80",
    "button.foreground": "#1B1E23",
    "button.hoverBackground": "#66D9EF",
    "dropdown.background": "#0A0A0A",
    "dropdown.border": "#ffffff22",
    "dropdown.foreground": "#ffffff99",
    "diffEditor.insertedTextBackground": "#00809B33",
    "editor.background": "#0A0A0A",
    "editor.foreground": "#F8F8F2",
    "list.errorForeground": "#FF5555",
    "errorForeground": "#FF5555",
    "list.warningForeground": "#FFAB40",
    "editorError.foreground": "#FF5555",
    "editorWarning.foreground": "#FFAB40",
    "editorRuler.foreground": "#ffffff66",
    "editorOverviewRuler.border": "#ff000000",
    "editorOverviewRuler.modifiedForeground": "#66D9EF00",
    "editorOverviewRuler.addedForeground": "#47ffa000",
    "editorOverviewRuler.deletedForeground": "#E040FB00",
    "editorOverviewRuler.errorForeground": "#FF5555",
    "editorOverviewRuler.warningForeground": "#FFAB40",
    "editorOverviewRuler.infoForeground": "#FFFF0000",
    "editorOverviewRuler.bracketMatchForeground": "#40C4FF00",
    "editorGutter.addedBackground": "#4ade80",
    "editorGutter.modifiedBackground": "#40C4FF",
    "editorGutter.deletedBackground": "#FF5555",
    "editor.lineHighlightBackground": "#ffffff10",
    "editor.lineHighlightBorder": "#ffffff05",
    "editor.selectionBackground": "#f8f8f259",
    "editor.selectionHighlightBorder": "#ffffff30",
    "editor.selectionHighlightBackground": "#f8f8f23b",
    "editor.wordHighlightBackground": "#ff000000",
    "editor.wordHighlightStrongBorder": "#ffffff80",
    "editor.inactiveSelectionBackground": "#ffffff20",
    "editorCursor.foreground": "#4ade80",
    "editor.findMatchBackground": "#40c3ff66",
    "editor.findMatchHighlightBackground": "#ffffff22",
    "editor.hoverHighlightBackground": "#ffffff22",
    "editorCodeLens.foreground": "#ffffff66",
    "editorGroup.emptyBackground": "#0c0c0c",
    "editorGroup.border": "#181A1F",
    "editorGroupHeader.tabsBackground": "#000",
    "editorIndentGuide.background1": "#ffffff22",
    "editorLineNumber.foreground": "#ffffff33",
    "editorWhitespace.foreground": "#ffffff66",
    "editorHoverWidget.background": "#1E1E1E",
    "editorHoverWidget.border": "#1E1E1E",
    "editorSuggestWidget.background": "#1E1E1E",
    "editorSuggestWidget.border": "#1E1E1E",
    "editorSuggestWidget.selectedBackground": "#2c313a",
    "editorWidget.background": "#1E1E1E",
    "focusBorder": "#F92672",
    "widget.shadow": "#00000090",
    "scrollbar.shadow": "#ff000000",
    "editorWidget.border": "#40C4FF00",
    "editorSuggestWidget.highlightForeground": "#40C4FF",
    "editorSuggestWidget.foreground": "#f8f8f0",
    "list.activeSelectionBackground": "#ffffff20",
    "list.activeSelectionForeground": "#fff",
    "list.focusBackground": "#ffffff22",
    "list.hoverBackground": "#ffffff10",
    "list.highlightForeground": "#fff",
    "list.inactiveSelectionBackground": "#ffffff22",
    "list.inactiveSelectionForeground": "#f8f8f0",
    "list.focusForeground": "#f8f8f0",
    "tree.indentGuidesStroke": "#ffffff20",
    "scrollbarSlider.background": "#ffffff33",
    "scrollbarSlider.activeBackground": "#ffffff99",
    "scrollbarSlider.hoverBackground": "#ffffff66",
    "sideBar.background": "#000",
    "sideBar.border": "#ffffff20",
    "sideBar.foreground": "#ffffff99",
    "sideBarTitle.foreground": "#f8f8f0",
    "sideBarSectionHeader.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#ffffff10",
    "statusBar.background": "#000",
    "statusBar.foreground": "#fff",
    "statusBarItem.hoverBackground": "#ffffff33",
    "statusBar.noFolderBackground": "#1E1E1E",
    "statusBar.debuggingBackground": "#1E1E1E",
    "terminal.background": "#000",
    "panel.border": "#141414",
    "panel.background": "#000",
    "panelTitle.activeBorder": "#F92672",
    "tab.border": "#ffffff00",
    "tab.activeBackground": "#000",
    "tab.activeBorder": "#ffffff20",
    "tab.inactiveBackground": "#000",
    "tab.activeForeground": "#fff",
    "tab.inactiveForeground": "#ffffff80",
    "tab.hoverBackground": "#000",
    "titleBar.activeBackground": "#1E1E1E",
    "titleBar.activeForeground": "#ffffff99",
    "titleBar.inactiveBackground": "#1E1E1E",
    "titleBar.inactiveForeground": "#ffffff99",
    "progressBar.background": "#4ade80",
    "input.background": "#0A0A0A",
    "input.foreground": "#f8f8f0",
    "input.border": "#F92672",
    "input.placeholderForeground": "#ffffff66",
    "inputOption.activeBorder": "#F92672",
    "inputOption.activeBackground": "#00000000",
    "inputValidation.infoBackground": "#0A0A0A",
    "inputValidation.infoBorder": "#F92672",
    "inputValidation.warningBackground": "#0A0A0A",
    "inputValidation.warningBorder": "#FFAB40",
    "inputValidation.errorBackground": "#0A0A0A",
    "inputValidation.errorBorder": "#FF5555",
    "pickerGroup.foreground": "#40C4FF",
    "pickerGroup.border": "#40c3ff00",
    "extensionButton.prominentBackground": "#4ade80",
    "extensionButton.prominentHoverBackground": "#66D9EF",
    "extensionButton.prominentForeground": "#0A0A0A",
    "editorLink.activeForeground": "#66D9EF"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#75715E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Orange constants and strings",
      "scope": [
        "constant.numeric",
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Yellow keywords and operators",
      "scope": [
        "keyword",
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "punctuation.section.embedded",
        "source.go keyword.package.go",
        "source.go keyword.import.go",
        "source.go keyword.function.go",
        "source.go keyword.type.go",
        "source.go keyword.const.go",
        "source.go keyword.var.go",
        "source.go keyword.map.go",
        "source.go keyword.channel.go",
        "source.go keyword.control.go",
        "source.cpp keyword.operator",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Blue storage and constants",
      "scope": [
        "storage.modifier",
        "meta.var.expr",
        "meta.class",
        "storage.type.class",
        "variable.other.constant.property.js",
        "variable.other.property.js",
        "source.go constant.language.go",
        "source.go constant.other.placeholder.go",
        "source.go variable",
        "variable.parameter.scala",
        "markup.italic.markdown",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Blue italics",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Pink variables and entities",
      "scope": [
        "variable",
        "entity.name.variable",
        "source.js",
        "entity.name",
        "entity.name.tag.js.jsx",
        "support.variable",
        "variable.other.object.property.js",
        "storage.type.cs",
        "storage.modifier.c",
        "storage.modifier.specifier.const.cpp",
        "meta.metadata.simple.clojure",
        "meta.metadata.map.clojure"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Green functions and tags",
      "scope": [
        "support.function",
        "meta.function",
        "entity.name.tag",
        "character.escape",
        "constant.character.escape",
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.from",
        "entity.other.attribute-name.id",
        "meta.function-call.generic.python",
        "variable.parameter.function-call.python",
        "constant.other.placeholder.c",
        "keyword.control.directive.include.c",
        "keyword.control.directive.define.c",
        "constant",
        "string.quoted",
        "string.template",
        "punctuation.definition.string",
        "constant.language.boolean.true"
      ],
      "settings": {
        "foreground": "#4ade80"
      }
    },
    {
      "name": "White text and attributes",
      "scope": [
        "constant.language",
        "keyword.other.unit",
        "support.function.misc.css",
        "punctuation.definition.tag",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "White text with italic",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#F8F8F2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Purple classes",
      "scope": ["entity.name.type", "meta.attribute.class.html"],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Red functions (Apple Intelligence Placeholder)",
      "scope": [
        "entity.name.function",
        "variable.function",
        "source.elixir entity.name.function"
      ],
      "settings": {
        "foreground": "#fc0303"
      }
    },
    {
      "name": "Green with bold",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#4ade80",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Green with italic",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4ade80"
      }
    },
    {
      "name": "Orange inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#D50"
      }
    },
    {
      "name": "Coral regex and colors",
      "scope": [
        "string.regexp",
        "punctuation.terminator.statement",
        "support.constant.font-name",
        "string.other.link.description",
        "string.interpolated.single.dart",
        "constant.language.boolean.false"
      ],
      "settings": {
        "foreground": "#f97e72"
      }
    },
    {
      "name": "Teal CSS properties and template expressions",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.json",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "switch-block.expr.js",
        "constant.other.placeholder.cpp",
        "keyword.control.directive.include.cpp",
        "keyword.control.directive.define.cpp",
        "source.go storage.type",
        "source.go keyword.struct.go",
        "source.go keyword.interface.go",
        "markup.underline.link.markdown",
        "markup.inline.raw.string.markdown",
        "punctuation.definition.quote.begin.markdown",
        "markup.quote.markdown",
        "variable.parameter.dart",
        "meta.template.expression.scala",
        "source.elixir punctuation.definition.string",
        "source.elixir variable.other.readwrite.module.elixir",
        "source.elixir variable.other.readwrite.module.elixir punctuation.definition.variable.elixir"
      ],
      "settings": {
        "foreground": "#72f1b8"
      }
    },
    {
      "name": "Pink object properties and headings",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "markup.heading",
        "entity.name.section",
        "beginning.punctuation.definition.list",
        "entity.name.variable.local.cs",
        "entity.name.variable.field.cs",
        "entity.name.variable.property.cs",
        "source.elixir .punctuation.binary.elixir",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#ff7edb"
      }
    },
    {
      "name": "Pink with bold",
      "scope": ["markup.heading.markdown", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#ff7edb",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Pink with italic",
      "scope": ["source.elixir .punctuation.binary.elixir"],
      "settings": {
        "foreground": "#ff7edb",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Light pink object properties",
      "scope": [
        "source.dart",
        "source.python",
        "source.scala",
        "source.go",
        "meta.symbol.clojure"
      ],
      "settings": {
        "foreground": "#ff7edbff"
      }
    },

    {
      "name": "Cyan Elixir and Clojure",
      "scope": [
        "source.elixir support.type.elixir",
        "source.elixir meta.module.elixir entity.name.class.elixir",
        "source.elixir constant.other.symbol.elixir",
        "source.elixir constant.other.keywords.elixir",
        "entity.global.clojure",
        "storage.control.clojure",
        "source.go meta.function-call.go"
      ],
      "settings": {
        "foreground": "#36f9f6"
      }
    },
    {
      "name": "Cyan with bold",
      "scope": ["entity.global.clojure"],
      "settings": {
        "foreground": "#36f9f6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Cyan with italic",
      "scope": ["storage.control.clojure"],
      "settings": {
        "foreground": "#36f9f6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Gray quote and separators",
      "scope": [
        "markup.quote",
        "punctuation.separator.key-value",
        "string.template meta.embedded.line"
      ],
      "settings": {
        "foreground": "#b6b1b1cc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Gray separators",
      "scope": ["punctuation.separator.key-value"],
      "settings": {
        "foreground": "#b6b1b1"
      }
    },
    {
      "name": "White-ish markup text",
      "scope": ["text.html", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#ffffffee"
      }
    },
    {
      "name": "Orange link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#D50"
      }
    },
    {
      "name": "C++ placeholder with italic",
      "scope": ["constant.other.placeholder.cpp"],
      "settings": {
        "foreground": "#72f1b8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C placeholder with italic",
      "scope": ["constant.other.placeholder.c"],
      "settings": {
        "foreground": "#4ade80",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Clojure macros with italic",
      "scope": ["meta.quoted-expression.clojure"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown with italic",
      "scope": [
        "markup.underline.link.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#72f1b8",
        "fontStyle": "italic"
      }
    }
  ]
}
